{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": [ "src/browser.ts", "src/node.ts" ],
	"tsconfig": "tsconfig.json",

	"out": "docs",
	"name": "full-utils API Docs",
	"readme": "README.md",
	"includeVersion": true,

	"exclude": [ "**/*.spec.ts", "**/dist/**", "**/*.js" ],
	"excludeExternals": true,
	"excludeInternal": true,
	"excludePrivate": true,
	"excludeNotDocumented": false,

	"categorizeByGroup": true,
	"searchInComments": true,

	"plugin": [],
	"githubPages": false,

	"externalSymbolLinkMappings": {
		"typescript": {
			"Date": "#",
			"Date.setMinutes": "#",
			"Date.getMinutes": "#",
			"String.padStart": "#",
			"Intl.DateTimeFormat": "#",
			"DataView": "#",
			"ArrayConstructor.isArray": "#",
			"parseStringLike": "#",
			"JSON.stringify": "#",
			"Error": "#",
			"AbortSignal": "#",
			"URL": "#",
			"Math.floor": "#"
		},
		"full-utils": {
			"parseStringLike": "#",
			"normalizeUnit": "#"
		}
	},

	"highlightLanguages": [ "ts", "js", "json", "bash", "regex" ]
}