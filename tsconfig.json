{
	"compilerOptions": {
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"declaration": true,
		"declarationMap": true,
		"emitDeclarationOnly": false,
		"sourceMap": true,
		"strict": true,
		"skipLibCheck": true,
		"outDir": "dist",
		"rootDir": "src",
		"lib": [ "ES2020", "DOM" ]
	},
	"include": [ "src" ]
}
